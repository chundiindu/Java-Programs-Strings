
class Solution 
{
    //Function to reverse words in a given string.
    String reverseWords(String S)
    {
        String[] words = S.split("\\.");
        String reverse = "";
        for(int i=words.length-1;i>=0;i--)
        {
            reverse+=words[i];
            if(i>0)
            {
                reverse += ".";
            }
        }
        return reverse.trim();
    }
}
